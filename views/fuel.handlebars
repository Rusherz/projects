
<div class="page-header">
    <h1 class="text-center">Citadel Fuel</h1>
</div>
<div class="row">
    <div class="col-md-10 col-md-offset-1">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Citadel Name</th>
                    <th>Location</th>
                    <th>Count Down</th>
                    <th>Date</th>
                </tr>
            </thead>
            <tbody>
                {{#each citadel}}
                <tr>
                    <td>{{this.Number}}</td>
                    <td>{{this.Name}}</td>
                    <td>{{this.Location}}</td>
                    <td>{{{this.TimeLeft}}}</td>
                    <td>{{{this.Date}}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>